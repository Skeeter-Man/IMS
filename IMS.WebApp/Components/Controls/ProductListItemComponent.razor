@inject                 NavigationManager NavigationManager

@if (Product is not null)
{
    <tr>
        <td>@Product.ProductName</td>
        <td>@Product.Quantity</td>
        <td>@Product.Price</td>
        <td>
            <a class="btn btn-primary"
               href="/editproduct/@Product.ProductId">
                Edit
            </a>
        </td>
        <td>
        </td>
    </tr>
}

@code {
    [Parameter]
    public Product? Product { get; set; }


}
